{
  "mrcleaner":
  {
    "all_products_container":
    {
      "type": "div",
      "attrs":
      {
        "data-type": "product-list"
      }
    },

    "products_container":
    {
      "type": "div",
      "attrs":
      {
        "data-type": "product",
        "class": "product-box gallery clearfix"
      }
    },

    "product_name":
    {
      "type": "a",
      "attrs":
      {
        "data-type": "product-url"
      }
    },

    "product_price_container":
    {
      "type": "div",
      "attrs":
      {
        "class": "product-price-container"
      }
    },

    "product_price":
    {
      "type": "span",
      "attrs":
      {
        "data-type": "field-price"
      }
    },

    "second_regular_price":
    {
      "type": "span",
      "attrs":
      {
        "class": "base-price"
      }
    },

    "discount_price":
    {
      "type": "span",
      "attrs":
      {
        "class": "discount-price"
      }
    },

    "product_url":
    {
      "type": "a",
      "attrs":
      {
        "data-type": "product-url"
      }
    },
    "available":
    {
      "type": "span",
      "attrs":
      {
        "class": "product-ias",
        "data-type": "field-ias"
      },
      "not_available_message": "Niedostępny"
    },

    "single_product_page":
    {
      "product_name":
      {
        "type": "span",
        "attrs":
        {
          "itemprop": "name"
        }
      },
      "product_price_container":
      {
        "type": "div",
        "attrs":
        {
          "class": "product-price-row"
        }
      },
      "product_price":
      {
        "type": "span",
        "attrs":
        {
          "class": "price",
          "data-type": "product-price"
        }
      },
      "second_regular_price":
      {
        "type": "span",
        "attrs":
        {
          "class": "base-price",
          "data-type": "product-base-price"
        }
      },
      "discount_price":
      {
        "type": "span",
        "attrs":
        {
          "class": "price new-price",
          "data-type": "product-price"
        }
      },
      "available":
      {
        "type": "div",
        "attrs":
        {
          "class": "product-unavailable-label"
        },
        "not_available_message": "Pozycja niedostępna"
      }
    }
  },

