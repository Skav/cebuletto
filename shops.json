{
  "mrcleaner":
  {
    "all_products_container":
    {
      "type": "div",
      "attrs":
      {
        "data-type": "product-list"
      }
    },

    "products_container":
    {
      "type": "div",
      "attrs":
      {
        "data-type": "product",
        "class": "product-box gallery clearfix"
      }
    },

    "product_name":
    {
      "type": "a",
      "attrs":
      {
        "data-type": "product-url"
      }
    },

    "product_price_container":
    {
      "type": "div",
      "attrs":
      {
        "class": "product-price-container"
      }
    },

    "product_price":
    {
      "type": "span",
      "attrs":
      {
        "class": "price"
      }
    },

    "product_discount_price":
    {
      "type": "span",
      "attrs":
      {
        "class": "discount-price"
      }
    },

    "product_url":
    {
      "type": "a",
      "attrs":
      {
        "data-type": "product-url"
      }
    }
  }
}